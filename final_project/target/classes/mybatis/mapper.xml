<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.KG.mybatis.myMapper">
	
	<select id="chkId" resultType="String">
		SELECT m_id FROM memberList WHERE m_id=#{m_id}
	</select>
	
	<insert id="addMember" parameterType="memberdto">
		INSERT INTO memberList values(#{m_id} , #{m_pw} , #{m_name} , #{m_date} , #{m_email})
	</insert>
</mapper>